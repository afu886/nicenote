{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "generate:css": {
      "dependsOn": ["^build"],
      "outputs": ["src/generated-tokens.css"]
    },
    "web#build": {
      "dependsOn": ["^build", "generate:css"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "test": {
      "dependsOn": ["^test"],
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "deploy": {
      "dependsOn": ["build"],
      "cache": false
    },
    "cf-typegen": {
      "cache": false
    },
    "nicenote-mobile#start": {
      "cache": false,
      "persistent": true
    },
    "nicenote-mobile#ios": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "nicenote-mobile#android": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "nicenote-desktop#start": {
      "cache": false,
      "persistent": true
    },
    "nicenote-desktop#macos": {
      "dependsOn": ["^build", "@nicenote/editor-bridge#build:template"],
      "cache": false
    },
    "nicenote-desktop#windows": {
      "dependsOn": ["^build", "@nicenote/editor-bridge#build:template"],
      "cache": false
    },
    "@nicenote/editor-bridge#build:template": {
      "outputs": ["src/assets/editor.html"],
      "dependsOn": ["^build"]
    }
  }
}
